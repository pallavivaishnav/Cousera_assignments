score = input("Enter Score: ")
score=float(score)
if(0.0<=score<=1.0):
    if(0.9<=score<=1.0):
        print("A")
    elif(0.8<=score<=0.9):
        print("B")
    elif(0.7<=score<=0.8):
        print("C")
    elif(0.6<=score<=0.7):
        print("D")
    else:
        print("F")
else:
    print("Enter values in between 0.0 to 1.0")
    exit()
